<% include partials/header %>

<div class="row">
    <h1 class="credentialH1">Sign Up</h1>
    <div class="credentialDiv">
        <form action="/register" method="POST">
            <div class="form-group">
                <input class="form-control" type="text" name="username" placeholder="username" disabled>
            </div>
            <div class="form-group">
                <input class="form-control" type="password" name="password" placeholder="password" disabled>
            </div>
            <div class="form-group">
                <button class="btn btn-lg btn-primary btn-block">Sign Up!</button>
            </div>
        </form> 
        <a class="registerLink" href="/campgrounds">Go Back</a>
    </div>
</div>

<% include partials/footer %>